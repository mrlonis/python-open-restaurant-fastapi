[run]
branch = True
include =
    ./app/*
omit =
    ./.idea/*
    ./.pytest_cache/*
    ./.vscode/*
    ./alembic/*
    ./htmlcov/*
    ./junit/*
    ./scripts/*
    ./tests/*
    */__pycache__/*
    ./setup.py
concurrency = thread,greenlet
[report]
precision = 2
show_missing = True
skip_empty = True
sort = Name
[html]
directory = ./htmlcov
skip_empty = True
title = python-open-restaurant-fastapi Coverage Report
[xml]
output = ./junit/coverage.xml
